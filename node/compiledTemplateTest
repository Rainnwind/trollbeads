    <content content-id="INT_halfblock_text_aboutus_theproducts_distributionandretailers">
        <display-name xml:lang="zh">About us - the products - Distribution and Retailers</display-name>
        <online-flag>true</online-flag>
        <searchable-flag>false</searchable-flag>
